{
	"name": "noresources/ns-php-http", 
	"description": "HTTP utilities", 
	"type": "library", 
	"license": "MIT", 
	"keywords": [
		"http", 
		"psr-7"
	], 
	"authors": [
		{
			"name": "Renaud Guillard", 
			"email": "dev@nore.fr", 
			"role": "lead"
		}
	], 
	"autoload": {
		"psr-4": {
			"NoreSources\\Http\\": "src/", 
			"NoreSources\\Http\\Test\\": "tests/Shared"
		}
	}, 
	"autoload-dev": {
		"psr-4": {
			"NoreSources\\Http\\Test\\": "tests/Private", 
			"NoreSources\\Http\\Tools\\": "tools/src"
		}
	}, 
	"require": {
		"php": ">=7.0", 
		"noresources/ns-php-mediatype": "^1.5", 
		"noresources/ns-php-data": "^2.0", 
		"noresources/ns-php-languagetag": "^1.1", 
		"psr/http-message": "~1.0", 
		"psr/http-server-middleware": "^1.0@dev"
	}, 
	"scripts": {
		"test": "@php  ./vendor/bin/phpunit --color=never --bootstrap vendor/autoload.php tests"
	}, 
	"require-dev": {
		"phpunit/phpunit": ">= 5.0", 
		"laminas/laminas-diactoros": "^2.3", 
		"laminas/laminas-httphandlerrunner": "^1.2@dev", 
		"nette/php-generator": "^3.4@dev"
	}, 
	"extra": {
		"branch-alias": {
			"dev-1.0.x": "1.0.x-dev", 
			"dev-1.1.x": "1.1.x-dev",
			"dev-1.2.x": "1.2.x-dev", 
			"dev-1.3.x": "1.3.x-dev", 
			"dev-master": "2.0.x-dev"
		}
	}
}
